from collections import deque

def bfs(numbers, target):
    q = deque([[0, 0]])
    ans = 0
    while q:
        i, total = q.popleft()
        if i < len(numbers):
            q.append([i + 1, total + numbers[i]])
            q.append([i + 1, total - numbers[i]])
        elif total == target:
            ans += 1
    return ans
        

def solution(numbers, target):
    return bfs(numbers, target)