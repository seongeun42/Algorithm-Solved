def dfs(i, numbers, total, target):
    ans = 0
    if len(numbers) == i:
        return 1 if total == target else 0
    ans += dfs(i + 1, numbers, total - numbers[i], target)
    ans += dfs(i + 1, numbers, total + numbers[i], target)
    return ans

def solution(numbers, target):
    answer = dfs(0, numbers, 0, target)
    return answer